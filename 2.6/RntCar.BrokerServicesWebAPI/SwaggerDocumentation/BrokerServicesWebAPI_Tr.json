          "brokerCode": {
            "description": "Broker için ayrılan benzersiz kod. Entegrasyon sırasında size ait kodu talep ediniz",
            "type": "string"
          },
          "langId": {
            "format": "int32",
            "description": "Dil kodu. İngilizce: 1033, Türkçe: 1055",
            "type": "integer"
          },
          "channelCode": {
            "format": "int32",
            "type": "integer"
          }
        }
      },
      "GetMasterDataResponse_Broker": {
