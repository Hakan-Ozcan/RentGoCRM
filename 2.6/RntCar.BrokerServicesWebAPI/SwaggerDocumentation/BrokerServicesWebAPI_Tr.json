          "groupCodeDescription": {
            "description": "Grup kod açıklaması. Örnek: Nissan Micra ve benzeri",
            "type": "string"
          },
          "transmissionName": {
            "description": "Vites tipi adı",
            "type": "string"
          },
          "fuelTypeName": {
            "description": "Yakıt tipi adı",
            "type": "string"
          },
          "findeksPoint": {
            "format": "int32",
