          "minimumAge": {
            "format": "int32",
            "description": "Araç grubunu kiralamak için gerekli minimum yaş sınırı",
            "type": "integer"
          },
          "minimumDriverLicense": {
            "format": "int32",
            "description": "Araç grubunu kiralamak için gerekli minimum sürücü belgesi yılı",
